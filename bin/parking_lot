#!/usr/bin/env ruby

project_bin_dir = File.join(File.dirname(File.expand_path(__FILE__)))
target_dir = File.join(project_bin_dir, '..', 'target')
if ARGV.length > 0
  	cmd = "java -jar #{target_dir}/parking-lot-gojek-1.0-SNAPSHOT.jar " + ARGV[0]
  else 
  	cmd = "java -jar #{target_dir}/parking-lot-gojek-1.0-SNAPSHOT.jar "
end

system cmd
